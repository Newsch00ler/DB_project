{% extends "__content.twig" %}

{% block content %}
<div class="album py-5">
    <div class="container">
        <h1>{{header}}</h1>
        <table class="table table-striped table-dark" style="text-align:center;">
            <thead>
                <tr>
                    {% for col_name in column_name %}
                        <th scope="col">{{col_name}}</th>
                    {% endfor %}
                </tr>
              </thead>
            <tbody>
                {% for object in objects %}
                    <tr>
                        <td>{{object[1]}}</td>
                        <td>{{object[2]}}</td>
                        <td>{{object[3]}}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
        {{nav|raw}}
    </div>
</div>   
{% endblock %}